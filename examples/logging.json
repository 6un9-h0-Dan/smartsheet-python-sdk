{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    }
  },
  "formatters": {
    "default": {
      "format": "%(levelname)s, in %(filename)s, line %(lineno)d: %(message)s"
    }
  },
  "root": {
      "level": "DEBUG",
      "handlers": ["console"]
  }
}
